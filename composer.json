{
	"name": "sitegeist/sms-cooluriclearcache",
	"type": "typo3-cms-extension",
	"description": "Adds a button to the clear cache menu for invalidating all cached links in CoolURI.",
	"version": "1.0.0",
	"homepage": "https://github.com/sitegeist/sms-cooluriclearcache",
	"license": ["GPL-2.0+"],
	"keywords": ["TYPO3 CMS", "url rewriting", "speaking url", "cooluri", "clear cache"],
	"authors": [
		{
			"name": "Markus Kobligk",
			"email": "kobligk@sitegeist.de",
			"homepage": "https://sitegeist.de",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/sitegeist/sms-cooluriclearcache/issues"
	},
	"require": {
		"typo3/cms-core": "^8.7.0"
	},
	"autoload": {
		"psr-4": {
            "SMS\\SmsCooluriclearcache\\": "Classes"
		}
	},
	"replace": {
		"sms_cooluriclearcache": "self.version",
		"typo3-ter/sms-cooluriclearcache": "self.version"
	}
}
